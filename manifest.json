{
    "manifest_version": 2,
    "name": "to_do list extention",
    "version": "0.1.0",
    "descriptin": "this is to_do list extention",
    "browser_action": {
        "default_title": "To_do List",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}